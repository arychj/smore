<div class="row">
    <div class="col s12">
        {% if error %}
            {% include 'partials/flash/flash.html.twig' with {
                'level': 'error',
                'message': (error is iterable and error.messageKey ? error.messageKey|trans(error.messageData, 'security') : error)
            } %}
        {% endif %}

        {% for label, messages in flashes %}
            {% for message in messages %}
                {% include 'partials/flash/flash.html.twig' with {'level': label, 'message': message} %}
            {% endfor %}
        {% endfor %}
    </div>
</div>